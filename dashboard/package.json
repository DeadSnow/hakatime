{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "Dashboard for Hakatime",
  "main": "index.js",
  "author": "Konstantinos Sideris <siderisk@auth.gr>",
  "license": "UNLICENSE",
  "private": true,
  "devDependencies": {
    "eslint": "^7.1.0",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-custom-dist-structure": "^1.1.9",
    "prettier": "1.19.1"
  },
  "scripts": {
    "dev": "parcel index.html --no-source-maps",
    "prod": "parcel build index.html --detailed-report --no-source-maps",
    "lint": "eslint --ext js src",
    "fmt": "prettier --write src/**/*.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "apexcharts": "^3.19.0",
    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "litepicker": "^1.3.4",
    "lodash": "^4.17.15",
    "mithril": "^2.0.4",
    "popper.js": "^1.16.1",
    "startbootstrap-sb-admin-2": "^4.0.7",
    "typeface-nunito": "^1.1.3"
  },
  "customDistStructure": {
    "options": {
      "development": true
    },
    "config": {
      ".css": "css",
      ".map": "css",
      ".js": "js",
      "images": [".jpg", ".svg", ".png"]
    }
  }
}
